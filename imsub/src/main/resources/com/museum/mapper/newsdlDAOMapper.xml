<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.museum.imsub.dao.NewsdlDAO">
    <select id="findAll" resultType="NewsId">
        select * from news
    </select>
    <select id="findByid" parameterType="int" resultType="NewsId">
         select * from news where nid = #{id}
    </select>
    <select id="findByname" parameterType="String" resultType="NewsId">
         select * from news where mname = #{name}
    </select>
</mapper>

